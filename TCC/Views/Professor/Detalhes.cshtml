@model Dominio.Professor

@{
    ViewBag.Title = "Detalhes";
}



<div class="container">
    <div class="row">
        <div class="form-horizontal col-sm-6">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title">Detalhes do Professor</h3>
                </div>
                <div class="panel-body">


                    <hr />
                    <div class="row">
                        <dl class="dl-horizontal">
                            <dt>
                                @Html.LabelFor(model => model.idProf, "ID do Professor")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.idProf)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.Nome, "Nome")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Nome)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.telProf, "Telefone")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.telProf)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.sexoProf, "Sexo")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.sexoProf)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.nascimentoProf, "Data de Nascimento")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.nascimentoProf)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.enderecoProf, "Endereco")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.enderecoProf)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.cpfProf, "Cpf")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.cpfProf)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.tipo, "Tipo")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.tipo)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.formacao, "Formação")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.formacao)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.senha, "Senha")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.senha)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.login, "login")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.login)
                            </dd>

                            <dt>
                                @Html.LabelFor(model => model.idTurma, "Turma")
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.idTurma)
                            </dd>

                        </dl>
                        <p>
                            @Html.ActionLink("Editar", "Editar", new { id = Model.idProf }) |
    @Html.ActionLink("Voltar a lista", "Visualizar")
                        </p>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
